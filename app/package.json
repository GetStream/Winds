{
  "name": "Winds",
  "version": "2.3.0",
  "description": "Winds is a beatiful open source RSS Reader and Podcast app. Wind 2.0 was created using React/Redux/Node.\nFor more information, visit https://getstream.io/winds/.\nTo contribute or run your own version head over to Github: https://github.com/getstream/winds",
  "private": true,
  "author": "Winds Team <winds@getstream.io>",
  "license": "BSD-3-Clause",
  "repository": "https://github.com/getstream/winds.git",
  "bugs": {
    "url": "https://github.com/getstream/winds/issues"
  },
  "homepage": "https://winds.getstream.io",
  "main": "public/electron.js",
  "keywords": [
    "Winds",
    "RSS",
    "RSS Reader",
    "Podcast",
    "Podcast Player",
    "Stream",
    "GetStream"
  ],
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "web": "concurrently \"yarn watch-css\" \"yarn react-scripts start\"",
    "build": "yarn build-css && react-scripts build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\" \"yarn watch-css\"",
    "pack": "yarn build-css && electron-builder --dir",
    "dist": "npx build --x64 --macos --win --linux --c.extraMetadata.main=build/electron.js -p always",
    "prettier": "prettier --config ../.prettierrc --write \"src/**/*.js\""
  },
  "dependencies": {
    "algoliasearch": "^3.30.0",
    "axios": "^0.18.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.2",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "electron-boilerplate": "^1.1.1",
    "electron-google-analytics": "^0.1.0",
    "electron-is": "^3.0.0",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.17",
    "electron-notifications": "^1.0.0",
    "electron-open-link-in-browser": "^1.0.2",
    "electron-publisher-s3": "^20.17.2",
    "electron-remote": "^1.3.0",
    "electron-updater": "^3.1.2",
    "eslint": "5.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "getstream": "^3.20.0",
    "github-buttons": "^0.0.1-security",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "install": "^0.12.1",
    "is-electron": "^2.1.0",
    "is-windows": "^1.0.2",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "node-notifier": "^5.2.1",
    "node-sass-chokidar": "^1.3.3",
    "raven-js": "^3.27.0",
    "rc-slider": "^8.6.2",
    "react": "^16.4.2",
    "react-audio-player": "^0.10.0",
    "react-dom": "^16.4.2",
    "react-dropzone": "^5.0.1",
    "react-github-button": "^0.1.11",
    "react-html-parser": "^2.0.2",
    "react-image": "^1.4.1",
    "react-modal": "^3.5.1",
    "react-player": "^1.6.4",
    "react-popover": "^0.5.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-waypoint": "^8.0.3",
    "redux": "^4.0.0",
    "sanitize-html": "^1.19.1",
    "stream-analytics": "^2.7.1",
    "video-react": "^0.13.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-osx-sign": "^0.4.10",
    "eslint": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "json": "^9.0.6",
    "prettier": "^1.14.2",
    "prettierrc": "^0.0.0-5",
    "react-scripts": "^1.1.5",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "version-bump-prompt": "^4.1.0",
    "wait-on": "^3.0.1"
  },
  "build": {
    "appId": "io.getstream.winds",
    "compression": "normal",
    "copyright": "Copyright Â© 2018 Stream.io Inc. All rights reserved.",
    "productName": "Winds",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "mac": {
      "icon": "assets/icon.icns",
      "type": "distribution",
      "target": [
        "pkg",
        "dmg",
        "mas"
      ],
      "publish": {
        "provider": "s3",
        "bucket": "winds-2.0-releases",
        "path": "releases"
      },
      "category": "public.app-category.news"
    },
    "mas": {
      "entitlements": "assets/entitlements.mas.plist",
      "entitlementsInherit": "assets/entitlements.mas.inherit.plist",
      "provisioningProfile": "../winds-private/embedded.provisionprofile"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico",
      "publish": {
        "provider": "s3",
        "bucket": "winds-2.0-releases",
        "path": "releases"
      }
    },
    "linux": {
      "icon": "assets/icon-256x256.png",
      "target": [
        "snap",
        "AppImage"
      ],
      "description": "Winds is a beautiful open-source RSS Reader and Podcast application provided by GetStream.io.",
      "category": "Network;Feed",
      "publish": {
        "provider": "s3",
        "bucket": "winds-2.0-releases",
        "path": "releases"
      }
    }
  },
  "files": [
    "*.js",
    "build",
    "node_modules"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
